{
  "name": "@riddance/env",
  "version": "0.9.1",
  "type": "module",
  "keywords": [
    "riddance",
    "environment",
    "devenv",
    "opinionated"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.14"
  },
  "dependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "^22",
    "cspell-lib": "9.2.0",
    "eslint": "9.33.0",
    "@eslint/js": "9.33.0",
    "globals": "16.3.0",
    "typescript-eslint": "8.40.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-prefer-arrow-functions": "3.6.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-unicorn": "60.0.0",
    "mocha": "11.7.1",
    "prettier": "3.6.2",
    "source-map-support": "0.5.21",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/eslint-plugin-prettier": "3.1.3",
    "@types/source-map-support": "0.5.10"
  },
  "scripts": {
    "prepare": "tsc && node ./bin/prepare.js",
    "start": "npm run prepare && node ./bin/install.js && node ./bin/watch.js",
    "prepack": "npm run prepare && node ./bin/build.js",
    "prepublishOnly": "tsc && node ./bin/prepublish.js",
    "postpublish": "node ./bin/sync.js",
    "postinstall": "node ./bin/install.js || (tsc && node ./bin/install.js)"
  },
  "files": [
    "/*.js",
    "**/lib/*.js",
    "/template/**",
    "/bin/install.js",
    "instructions.md"
  ],
  "bin": {
    "riddance-watch": "./bin/watch.js",
    "riddance-build": "./bin/build.js",
    "riddance-fix": "./bin/fix.js",
    "riddance-prepublish": "./bin/prepublish.js",
    "riddance-sync": "./bin/sync.js"
  },
  "ai": {
    "instructions": "./instructions.md"
  }
}
